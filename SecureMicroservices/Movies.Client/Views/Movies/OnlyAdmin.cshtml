@using Movies.Client.Models;

@{
    ViewData["Title"] = "OnlyAdmin";
}

<h1>OnlyAdmin</h1>
<table class="table">
    <thead>
        <tr>
            <th>Claim Type</th>
            <th>Claim Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model.UserInfoDictionary)
        {
            <tr>
                <td>
                    @item.Key
                </td>
                <td>
                    @item.Value
                </td>
            </tr>
        }
    </tbody>
</table>